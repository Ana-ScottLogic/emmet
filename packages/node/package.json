  },
  "homepage": "https://github.com/emmetio/node#readme",
  "devDependencies": {
    "mocha": "^6.1.4",
    "rollup": "^1.16.2",
    "rollup-plugin-typescript2": "^0.21.2",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "mocha": {
    "require": "ts-node/register",
