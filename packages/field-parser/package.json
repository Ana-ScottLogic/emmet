{
  "name": "@emmetio/field-parser",
  "version": "0.2.0",
  "description": "Parses snippet fields (aka tab-stops) like `${1:foo}` in given string",
  "main": "./dist/field-parser.cjs.js",
  "module": "./dist/field-parser.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "tslint ./src/*.ts",
    "test": "mocha",
    "build": "rollup -c",
    "clean": "rm -rf ./dist",
    "prepare": "npm run lint && npm test && npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emmetio/emmet.git"
  },
  "keywords": [
    "emmet",
    "field",
    "tabstop"
  ],
  "author": "Sergey Chikuyonok <serge.che@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emmetio/emmet/issues"
  },
  "homepage": "https://github.com/emmetio/emmet#readme",
  "devDependencies": {
    "mocha": "^6.1.4",
    "rollup": "^1.16.2",
    "rollup-plugin-typescript2": "^0.21.2",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@emmetio/scanner": "^0.0.2"
  },
  "mocha": {
    "require": "ts-node/register",
    "spec": "./test/*.ts"
  }
}
